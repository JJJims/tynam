: '六年级六班'\n      }],\n      dataList: [],\n      searchInput: '',\n      currentRow: 0,\n      currentPage: 'home',\n      showAddDialog: false,\n      showDeleteDialog: false,\n      isEditor: false,\n      studentInfo: {\n        code: '',\n        name: '',\n        sex: '',\n        grader: ''\n      }\n    };\n  },\n  computed: {\n    currentData: function currentData() {\n      if (!this.dataList || this.dataList <= 0) {\n        return undefined;\n      }\n\n      var currentRow = this.currentRow;\n      var currentData = this.dataList.find(function (item, index) {\n        return index === currentRow;\n      });\n      return currentData;\n    },\n    nextId: function nextId() {\n      if (this.studentSource && this.studentSource.length > 0) {\n        return this.studentSource[this.studentSource.length - 1].id + 1;\n      }\n\n      return 0;\n    }\n  },\n  methods: {\n    search: function search() {\n      this.refreshDataSource();\n    },\n    refreshDataSource: function refreshDataSource() {\n      var filter = this.searchInput.toUpperCase();\n      var result = this.studentSource.filter(function (data) {\n        if (data.code.toUpperCase().indexOf(filter) > -1) {\n          return true;\n        } else if (data.name.toUpperCase().indexOf(filter) > -1) {\n          return true;\n        } else if (data.sex.toUpperCase().indexOf(filter) > -1) {\n          return true;\n        } else if (data.grader.toUpperCase().indexOf(filter) > -1) {\n          return true;\n        }\n\n        return false;\n      });\n      this.dataList = result;\n    },\n    rowClick: function rowClick(index) {\n      this.currentRow = index;\n    },\n    handleDelete: function handleDelete() {\n      this.showDeleteDialog = true;\n    },\n    handleEditor: function handleEditor() {\n      var currentData = this.currentData;\n\n      if (currentData) {\n        this.studentInfo.code = currentData.code;\n        this.studentInfo.name = currentData.name;\n        this.studentInfo.sex = currentData.sex;\n        this.studentInfo.grader = currentData.grader;\n        this.showAddDialog = true;\n        this.isEditor = true;\n      }\n    },\n    handleAdd: function handleAdd() {\n      this.showAddDialog = true;\n      this.isEditor = false;\n    },\n    handleAddDialogSave: function handleAddDialogSave() {\n      var validateResult = this.validate();\n\n      if (!validateResult) {\n        return false;\n      }\n\n      if (this.isEditor) {\n        var currentData = this.currentData;\n        var oldItem = this.studentSource.find(function (item) {\n          return item.id === currentData.id;\n        });\n\n        if (oldItem) {\n          oldItem.code = this.studentInfo.code;\n          oldItem.name = this.studentInfo.name, oldItem.sex = this.studentInfo.sex;\n          oldItem.grader = this.studentInfo.grader;\n        }\n      } else {\n        var data = {\n          code: this.studentInfo.code,\n          name: this.studentInfo.name,\n          sex: this.studentInfo.sex,\n          grader: this.studentInfo.grader\n        };\n        data.id = this.nextId;\n        this.studentSource.push(data);\n      }\n\n      this.handleAddDialogCancel();\n      this.refreshDataSource();\n    },\n    handleAddDialogCancel: function handleAddDialogCancel() {\n      this.studentInfo.code = '';\n      this.studentInfo.name = '';\n      this.studentInfo.sex = '';\n      this.studentInfo.grader = '';\n      this.showAddDialog = false;\n    },\n    handleDeleteDialogSave: function handleDeleteDialogSave() {\n      if (this.currentData) {\n        var dataId = this.currentData.id;\n        var result = this.studentSource.filter(function (item) {\n          return item.id !== dataId;\n        });\n        this.studentSource = result;\n        this.showDeleteDialog = false;\n        this.refreshDataSource();\n      }\n    },\n    handleDeleteDialogCancel: function handleDeleteDialogCancel() {\n      this.showDeleteDialog = false;\n    },\n    pageSelect: function pageSelect(page) {\n      this.currentPage = page;\n    },\n    validate: function validate() {\n      if (!this.studentInfo.name || !this.studentInfo.code) {\n        alert(\"姓名和学号不能为空\");\n        return false;\n      }\n\n      return true;\n    },\n    logOut: function logOut() {\n      util.clearCookie('loginSatus');\n      this.$router.push(\"/\");\n    }\n  },\n  created: function created() {\n    this.refreshDataSource();\n  }\n};",{"version":3,"sources":["HomePage.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuFA,OAAA,IAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,UADA;AAEA,EAAA,IAAA,EAAA,gBAAA;AACA,WAAA;AACA,MAAA,aAAA,EAAA,CACA;AAAA,QAAA,EAAA,EAAA,CAAA;AAAA,QAAA,IAAA,EAAA,OAAA;AAAA,QAAA,IAAA,EAAA,KAAA;AAAA,QAAA,GAAA,EAAA,GAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,EAAA,EAAA,CAAA;AAAA,QAAA,IAAA,EAAA,OAAA;AAAA,QAAA,IAAA,EAAA,KAAA;AAAA,QAAA,GAAA,EAAA,GAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAFA,EAGA;AAAA,QAAA,EAAA,EAAA,CAAA;AAAA,QAAA,IAAA,EAAA,OAAA;AAAA,QAAA,IAAA,EAAA,IAAA;AAAA,QAAA,GAAA,EAAA,GAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAHA,EAIA;AAAA,QAAA,EAAA,EAAA,CAAA;AAAA,QAAA,IAAA,EAAA,OAAA;AAAA,QAAA,IAAA,EAAA,KAAA;AAAA,QAAA,GAAA,EAAA,GAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAJA,CADA;AAOA,MAAA,QAAA,EAAA,EAPA;AAQA,MAAA,WAAA,EAAA,EARA;AASA,MAAA,UAAA,EAAA,CATA;AAUA,MAAA,WAAA,EAAA,MAVA;AAWA,MAAA,aAAA,EAAA,KAXA;AAYA,MAAA,gBAAA,EAAA,KAZA;AAaA,MAAA,QAAA,EAAA,KAbA;AAcA,MAAA,WAAA,EAAA;AACA,QAAA,IAAA,EAAA,EADA;AAEA,QAAA,IAAA,EAAA,EAFA;AAGA,QAAA,GAAA,EAAA,EAHA;AAIA,QAAA,MAAA,EAAA;AAJA;AAdA,KAAA;AAqBA,GAxBA;AAyBA,EAAA,QAAA,EAAA;AACA,IAAA,WAAA,EAAA,uBAAA;AACA,UAAA,CAAA,KAAA,QAAA,IAAA,KAAA,QAAA,IAAA,CAAA,EAAA;AACA,eAAA,SAAA;AACA;;AACA,UAAA,UAAA,GAAA,KAAA,UAAA;AACA,UAAA,WAAA,GAAA,KAAA,QAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,eAAA,KAAA,KAAA,UAAA;AACA,OAFA,CAAA;AAGA,aAAA,WAAA;AACA,KAVA;AAWA,IAAA,MAAA,EAAA,kBAAA;AACA,UAAA,KAAA,aAAA,IAAA,KAAA,aAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,KAAA,aAAA,CAAA,KAAA,aAAA,CAAA,MAAA,GAAA,C